<div class="container">
	<div class="alert alert-warning alert-dismissible fade show my-4" role="alert">
	  <% flash.each do |name,msg| %>
	  	<%= msg %>
	  <%end%>
	  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
	    <span aria-hidden="true">&times;</span>
	  </button>
	</div>
</div>
